{
  "name": "dawapack/php-chassis-framework",
  "description": "PHP Chassis Framework - a lightweight framework used in all DaWaPack projects",
  "keywords": ["dawapack", "php", "chassis", "framework"],
  "license": "Apache-2.0",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dawapack/asyncapi-contract-sample.git"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-parallel": "*",
    "ext-pcntl": "*",
    "ext-simplexml": "*",
    "ext-zlib": "*",
    "dawapack/asyncapi-contract-sample": "^0",
    "league/config": "^1.1",
    "league/container": "^4.2",
    "monolog/monolog": "^2.3.0",
    "opis/json-schema": "^2.3",
    "php-amqplib/php-amqplib": "^3.0",
    "ramsey/uuid": "^4.2",
    "spatie/data-transfer-object": "^2.8.0",
    "symfony/yaml": "^5.4",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5.10",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "files": [
      "src/Helpers/collections.php",
      "src/Helpers/general.php"
    ],
    "psr-4": {
      "Chassis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chassis\\Tests\\": "tests/"
    },
    "files": [
      "src/Helpers/collections.php",
      "src/Helpers/general.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
